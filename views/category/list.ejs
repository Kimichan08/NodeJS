<%-include('../layout/header') %> 
<div class="container">
    <a href="/category/add" class="btn btn-success">Them san pham</a>
    <table class="table">
        <thead>
            <tr>
                <th>STT</th>
                <th>Name</th>
                <th>Status</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            <% category.forEach((item,index) => {%>
                <tr>
                    <td scope="row"><%= index + 1 %> </td>
                    <td><%= item.name %> </td>
                    <td><%=(item.status==1) ? 'Hien' : 'An' %> </td>
                    <td>
                        <a href="/category/edit/<%=item.id %> " class="btn btn-primary">Edit</a>
                        <a href="delete/<%=item.id %> " class="btn btn-danger">Delete</a>
                    </td>
                </tr>
            <%}); %> 
        </tbody>
    </table>
</div>
<%-include('../layout/footer') %> 